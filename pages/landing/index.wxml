<!--pages/landing/index.wxml-->


<view class="pics">
    <image class="images" mode="widthFix" src="/images/landing1.jpeg"></image>
    <image class="images" mode="widthFix" src="/images/landing2.jpeg"></image>
    <image class="images" mode="widthFix" src="/images/landing3.jpeg"></image>
    <view class="clear-container" >
        <view class="dark-square"></view>
        <image class="landing-page-logo" src="/images/logo.png"></image>
    </view>
<view class="index">{{venues.index}}</view>
    <button class="btn-enter" bindtap="enterIndex">Enter</button>
</view>



